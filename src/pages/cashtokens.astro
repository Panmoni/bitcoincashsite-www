---
import Layout from '~/layouts/PageLayout.astro';

import InnerTitle from '~/components/widgets/InnerTitle.astro';

const metadata = {
  title: 'BCH CashTokens: Fungible Tokens and NFTs on Bitcoin Cash',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>

  <!-- InnerTitle Widget ******************* -->

  <InnerTitle>
    <Fragment slot="title">
      About Cash Tokens
    </Fragment>

    <Fragment slot="subtitle">
      Cash Tokens is a major upgrade for BCH in 2023
    </Fragment>
  </InnerTitle>

  <div
  class="mx-auto px-6 sm:px-6 max-w-3xl prose prose-lg lg:prose-xl dark:prose-invert dark:prose-headings:text-slate-300 prose-md prose-headings:font-heading prose-headings:leading-tighter prose-headings:tracking-tighter prose-headings:font-bold prose-a:text-primary dark:prose-a:text-blue-400 prose-img:rounded-md prose-img:shadow-lg mt-8 mb-8"
>

<p>Coming soon.</p>




</div>
  
</Layout>
